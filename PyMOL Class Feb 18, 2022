average_list = []
derivative_list = []
for index in range(len(temp_list)-1):
    temp1 = temp_list[index]
    temp2 = temp_list[index+1]
    int1 = int_list[index]
    int2 = int_list[index+1]
    derivative = (int2 - int1)/(temp2-temp1)
    average_temp = (temp1 + temp2)/2
    average_list.append(average_temp)
    derivative_list.append(derivative)
   
    
max_derivative = max(derivative_list)
max_index = derivative_list.index(max_derivative)
tm = average_list[30]
#30 pulls out the 30th item in list (starting from 0), which is where the max deriv is)
#print ('The average temp is', average_temp, ', and the max deriv is', max_derivative )
#max_derivative = 395? is this correct?
print('The Tm is', tm) 
#temperature at the maximum value of the peak
